{
  "name": "react-ui-tour",
  "version": "0.9.6",
  "description": "react tours library",
  "repository": "https://github.com/skbkontur/react-ui-tour.git",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "node build.prod.js",
    "build-doc": "webpack -p",
    "publish-doc": "rimraf dist && git clone -b gh-pages git@github.com:skbkontur/react-ui-tour.git dist && npm run build-doc && cd dist && git add --all && git commit --allow-empty -m \"published doc\" && git push && cd ..",
    "cmbuild": "npm i && npm run build",
    "storybook": "start-storybook -p 6006",
    "prepublish": "npm run test && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0 || ^16.0.0",
    "react-dom": "^0.14.0 || ^15.0.0-0 || ^16.0.0",
    "@skbkontur/react-ui": "<1.0.0",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@extern/tslint": "^1.0.1",
    "@skbkontur/react-ui": "^0.27.0",
    "@storybook/react": "~3.1.7",
    "@storybook/ui": "~3.1.7",
    "@types/enzyme": "^3.1.6",
    "@types/jest": "^21.1.8",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "awesome-typescript-loader": "^3.4.1",
    "classnames": "^2.2.5",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-14": "^1.0.5",
    "es3ify-webpack-plugin": "0.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-cli": "^22.4.2",
    "jest-css-modules-transform": "^1.0.3",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "react": "^15.6.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.6.2",
    "style-loader": "^0.16.1",
    "ts-jest": "^21.2.4",
    "tslib": "^1.8.0",
    "tslint": "^5.5.0",
    "typescript": "^3.0.3",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "1.13.2",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dev-server": "^1.16.5"
  },
  "keywords": [],
  "author": "Vladimir Tolstikov",
  "license": "ISC",
  "files": [
    "build/"
  ],
  "jest": {
    "setupFiles": [
      "./setupTests.ts"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      ".+\\.(css|less)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts"
    },
    "testURL": "http://localhost/"
  },
  "dependencies": {
    "raf": "^3.4.0"
  }
}
